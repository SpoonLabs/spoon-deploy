<settings>
  <servers>
    <server>
      <id>ossrh</id>
      <username>${env.SONATYPE_USER}</username>
      <password>${env.SONATYPE_PASSWORD}</password>
    </server>
  </servers>
  <profiles>
      <profile>
          <activation>
              <activeByDefault>true</activeByDefault>
          </activation>
          <properties>
              <gpg.passphrase>${env.SPOONBOT_PASSPHRASE}</gpg.passphrase>
          </properties>
      </profile>
  </profiles>
</settings>
